// EXCEL (.xlsx)

try {
  $writer = new XLSXWriter();
  $writer->writeSheetHeader('Sheet1', $header);
  foreach($data as $row) {  
    $writer->writeSheetRow('Sheet1', $row);
  }
  
  $writer->setTitle('Titulo');
  $writer->writeToFile('files/download/'.$file_name.'.xlsx' );
} catch (\Throwable $th) {
  throw $th;
}
